{
  "name": "@aether-identity/prisma",
  "version": "1.0.0",
  "description": "Prisma database package for Aether Identity",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "prisma generate --watch",
    "generate": "prisma generate",
    "push": "prisma db push",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy:prod": "prisma migrate deploy --name init",
    "studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "seed:default": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432/aether_identity?schema=public tsx prisma/seed.ts",
    "validate": "prisma validate",
    "format": "prisma format",
    "clean": "rimraf ./node_modules ./dist",
    "build": "prisma generate && npm run build:types",
    "build:types": "tsc --project tsconfig.build.json",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push --accept-data-loss",
    "db:pull": "prisma db pull",
    "db:studio": "prisma studio",
    "test": "tsx prisma/test.ts",
    "test:db": "tsx test-db-connection.ts",
    "test:db:watch": "tsx watch test-db-connection.ts",
    "lint": "eslint prisma/**/*.ts --fix",
    "precommit": "prisma format && prisma validate && npm run generate"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "aether-identity": "^1.0.2",
    "aether-vault": "^1.0.12",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.3",
    "config": "link:@prisma/client/config",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.4",
    "redis": "^5.11.0",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^25.2.3",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "eslint": "^10.0.0",
    "eslint-config-prisma": "^0.6.0",
    "prisma": "^7.4.0",
    "rimraf": "^6.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=5.6.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "prisma",
    "database",
    "aether-identity",
    "postgresql",
    "orm",
    "sql",
    "migration",
    "seeding"
  ],
  "author": "Sky Genesis Enterprise",
  "license": "MIT",
  "homepage": "https://github.com/skygenesisenterprise/aether-identity/tree/main/prisma",
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/aether-identity.git",
    "directory": "prisma"
  },
  "bugs": {
    "url": "https://github.com/skygenesisenterprise/aether-identity/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./seed": "./dist/seed.js",
    "./types": "./dist/types.d.ts"
  },
  "files": [
    "dist",
    "schema.prisma",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
