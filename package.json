{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "ts-node-dev --respawn --transpile-only api/src/server.ts",
    "build": "next build",
    "build:api": "tsc --project api/tsconfig.json",
    "start": "next start -H 0.0.0.0",
    "start:api": "node api/dist/server.js",
    "lint": "eslint",
    "db:select": "./scripts/select-prisma-schema.sh",
    "db:generate": "./scripts/select-prisma-schema.sh generate",
    "db:validate": "./scripts/select-prisma-schema.sh validate",
    "db:push": "./scripts/select-prisma-schema.sh generate && prisma db push --schema api/prisma/schema.prisma",
    "db:migrate": "./scripts/select-prisma-schema.sh generate && prisma migrate dev --schema api/prisma/schema.prisma",
    "db:migrate:deploy": "./scripts/select-prisma-schema.sh generate && prisma migrate deploy --schema api/prisma/schema.prisma",
    "db:studio": "./scripts/select-prisma-schema.sh generate && prisma studio --schema api/prisma/schema.prisma",
    "db:reset": "./scripts/select-prisma-schema.sh generate && prisma migrate reset --schema api/prisma/schema.prisma --force",
    "db:seed": "node api/dist/scripts/seed.js",
    "db:setup": "npm run db:generate && npm run db:migrate:deploy && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/swagger-ui-react": "^5.18.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "morgan": "^1.10.1",
    "next": "16.0.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.3.1",
    "react-inspector": "^9.0.0",
    "speakeasy": "^2.0.0",
    "swagger-ui-dist": "^5.30.3",
    "swagger-ui-react": "^5.30.3",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "twilio": "^5.10.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.8",
    "tailwindcss": "^4.1.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
