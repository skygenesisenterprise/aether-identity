{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "ts-node-dev --respawn --transpile-only api/src/server.ts",
    "build": "next build",
    "build:api": "tsc --project api/tsconfig.json",
    "start": "next start",
    "start:api": "node api/dist/server.js",
    "lint": "eslint",
    "db:generate": "prisma generate --schema api/prisma/schema.prisma",
